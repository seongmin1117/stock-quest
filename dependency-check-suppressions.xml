<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Stock Quest OWASP Dependency Check Suppressions -->

    <!-- Spring Boot Framework False Positives -->
    <!-- These are often flagged but are generally managed by Spring Boot BOM -->
    <suppress>
        <notes><![CDATA[
        Spring Boot managed dependencies are regularly updated through BOM version management.
        False positive for older CVEs that don't apply to current version.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>

    <!-- Test Dependencies - Not deployed to production -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not included in production builds.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/.*test.*$</packageUrl>
        <cpe regex="true">.*</cpe>
    </suppress>

    <!-- Development Only Dependencies -->
    <suppress>
        <notes><![CDATA[
        Development tools and devDependencies are not included in production builds.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/.*@types/.*$</packageUrl>
        <cpe regex="true">.*</cpe>
    </suppress>

    <!-- Known False Positives for Specific Libraries -->
    <suppress>
        <notes><![CDATA[
        Jackson databind - Managed by Spring Boot BOM and regularly updated.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/jackson-databind.*$</packageUrl>
        <vulnerabilityName regex="true">CVE-2020-.*</vulnerabilityName>
    </suppress>

    <!-- MySQL Connector False Positives -->
    <suppress>
        <notes><![CDATA[
        MySQL Connector J - Using latest version, old CVEs don't apply.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.mysql/mysql-connector-j.*$</packageUrl>
        <cve>CVE-2022-21363</cve>
    </suppress>

    <!-- Lombok - Compile time only -->
    <suppress>
        <notes><![CDATA[
        Lombok is compile-time only and not included in runtime.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.projectlombok/lombok.*$</packageUrl>
        <cpe regex="true">.*</cpe>
    </suppress>

    <!-- Node.js and NPM Development Dependencies -->
    <suppress>
        <notes><![CDATA[
        ESLint and related development tools are not deployed to production.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/eslint.*$</packageUrl>
        <cpe regex="true">.*</cpe>
    </suppress>

    <!-- Jest Testing Framework -->
    <suppress>
        <notes><![CDATA[
        Jest and testing libraries are development dependencies only.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/jest.*$</packageUrl>
        <cpe regex="true">.*</cpe>
    </suppress>

    <!-- Playwright Testing Framework -->
    <suppress>
        <notes><![CDATA[
        Playwright is used for E2E testing only, not in production.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/@playwright/.*$</packageUrl>
        <cpe regex="true">.*</cpe>
    </suppress>

    <!-- Next.js Framework Managed Dependencies -->
    <suppress>
        <notes><![CDATA[
        Next.js manages its dependencies and security updates through framework updates.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/next/.*$</packageUrl>
        <vulnerabilityName regex="true">CVE-2021-.*</vulnerabilityName>
    </suppress>

    <!-- H2 Database - Test Only -->
    <suppress>
        <notes><![CDATA[
        H2 database is used for testing only, not in production.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.h2database/h2.*$</packageUrl>
        <cpe regex="true">.*</cpe>
    </suppress>

    <!-- Netty DNS Native macOS - Platform Specific -->
    <suppress>
        <notes><![CDATA[
        Netty DNS native for macOS is platform-specific optimization, not a security risk.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.netty/netty-resolver-dns-native-macos.*$</packageUrl>
        <cpe regex="true">.*</cpe>
    </suppress>
</suppressions>